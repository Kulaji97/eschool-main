@model IEnumerable<ESCHOOLING.Shared.Models.Events>
@{
    Layout = "_TeacherLayout";
}

<!DOCTYPE html>
<html lang="en" dir="">

<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width,initial-scale=1" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <title>Dashboard </title>
    <link href="https://fonts.googleapis.com/css?family=Nunito:300,400,400i,600,700,800,900" rel="stylesheet" />
    <link href="../../dist-assets/css/themes/lite-purple.min.css" rel="stylesheet" />
    <link href="../../dist-assets/css/plugins/perfect-scrollbar.min.css" rel="stylesheet" />

    <script src="~/js/Search.js"></script>
</head>

<body class="text-left">

    <div class="main-content">
        <div class="breadcrumb">
            @*<h1 class="mr-2">Version 1</h1>*@
            <ul>
                <li><a href="">View Events List</a></li>
                <li>Student</li>
                <li>
                    <input type="button" class="btn btn-success" value="Export to Excel" onclick="location.href='@Url.Action("ExportToExcel", "Admin")'" />
                </li>
            </ul>
           
        </div>
        <div class="col-lg-12">
            <div class="row">
                <div class="table-responsive">
                    <table class="table table-striped">
                        <thead>
                            <tr>
                                <th scope="col">#</th>
                                <th scope="col">EventName</th>
                                <th scope="col">Date</th>
                                <th scope="col">Time</th>
                                <th scope="col">Place</th>
 
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <th scope="row">
                                        @Html.DisplayFor(modelItem => item.Id)
                                    </th>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.EventName)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Date)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Time)
                                    </td>
                                 
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Place)
                                    </td>
        
                                    @*<td>
                                <a class="text-success mr-2" href="#"><i class="nav-icon i-Pen-2 font-weight-bold"></i></a><a class="text-danger mr-2" href="#"><i class="nav-icon i-Close-Window font-weight-bold"></i></a>
                                </td>*@
                                </tr>
                            }

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
        <!-- end of main-content -->
    </div>
</body>

</html>