@{
    Layout = "_TeacherLayout";
}

<!DOCTYPE html>
<html lang="en" dir="">


<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Sign Up | ECOM</title>
    <link href="https://fonts.googleapis.com/css?family=Nunito:300,400,400i,600,700,800,900" rel="stylesheet">
    <link href="../../dist-assets/css/themes/lite-purple.min.css" rel="stylesheet">

    <script src="~/js/RegisterDoc.js"></script>
    <!-- Sweet alert -->
    <script src="~/js/plugins/sweetalert/sweetalert.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.3/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>

    <!-- Sweet alert -->
    <script src="~/js/plugins/sweetalert/sweetalert.min.js"></script>

</head>


<body class="text-left">

    <!-- ============ Body content start ============= -->
    <div class="main-content">
        <div class="breadcrumb">
            @*<h1 class="mr-2">Version 1</h1>*@
            <ul>
                <li><a href="">Add Students</a></li>
                <li>Teacher</li>
            </ul>
        </div>
        <div class="col-lg-6">

            <div class="form-group">
                <label for="username"> name</label>
                <input class="form-control form-control-rounded" id="userName" name="userName" type="text" required>
            </div>
            <div class="form-group">
                <label for="email">Email address</label>
                <input class="form-control form-control-rounded" id="email" name="email" type="email" required>
            </div>
            <div class="form-group">
                <label for="password">Password</label>
                <input class="form-control form-control-rounded" id="password" name="password" type="password" required>
            </div>
            <div class="form-group">
                <label for="MobileNo">Mobile No</label>
                <input class="form-control form-control-rounded" id="mobileNo" name="mobileNo" type="text" required>
            </div>
            <div class="form-group">
                <label for="Address">Address</label>
                <input class="form-control form-control-rounded" id="address" name="address" type="text" required>
            </div>
            <div class="form-group">
                <label for="Address">Grade</label>
                <input class="form-control form-control-rounded" id="grade" name="grade" type="text" required>
            </div>
            <button class="btn btn-primary btn-block btn-rounded mt-2 col-lg-4 float-right" onclick="StudentRegistrationBtn_Click(2);"> Add</button>

        </div>
        <!-- end of main-content -->
    </div>

</body>

</html>
