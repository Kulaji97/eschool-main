                            <label for="MobileNo">Mobile No</label>
                            <input class="form-control form-control-rounded" id="mobileNo" name="mobileNo" type="text" required>
                        </div>
                        <div class="form-group">
                            <label for="Address">Address</label>
                            <input class="form-control form-control-rounded" id="address" name="address" type="text" required>
                        </div>
                        <button class="btn btn-primary btn-block btn-rounded mt-2" onclick="UserRegistrationBtn_Click(1);">Sign Up</button>

                        <div class="mt-3 text-center">
                            <a class="text-muted" href="https://localhost:7092/Auth/Login">
